{
  "name": "whalli",
  "version": "1.0.0",
  "private": true,
  "description": "Turborepo monorepo with Next.js, NestJS, and shared packages",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --parallel",
    "lint": "turbo lint",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo build --filter=!./packages/eslint-config --filter=!./packages/prettier-config && changeset publish"
  },
  "dependencies": {
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@prisma/client": "^5.10.2",
    "bcrypt": "^5.1.1",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.24",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "turbo": "^2.5.8",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "pnpm": {
    "overrides": {
      "pdfjs-dist": "4.7.76",
      "pdf-parse": "1.1.1"
    }
  }
}